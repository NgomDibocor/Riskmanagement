<aura:component access="global">
    <ltng:require 
		    styles="{!$Resource.noUiSlider + '/nouislider.min.css'}"
		    scripts="{!$Resource.noUiSlider + '/nouislider.min.js'}"
		    afterScriptsLoaded="{!c.jsLoaded}" />
    
    <aura:attribute name="idAssessment" type="String"/>
    <aura:attribute name="label" type="String"/>
    <aura:attribute name="label2" type="String"/>
    <aura:attribute name="filterName" type="String"/>
    <aura:attribute name="min" type="String" default="0"/>
    <aura:attribute name="max" type="String" default="100"/>
    <aura:attribute name="step" type="String" default="1"/>
    
    <aura:attribute name="showProbilityRanking" type="boolean" default="true"/>
    <aura:attribute name="showHsseImpactsRanking" type="boolean" default="false"/>
    <aura:attribute name="showBusinessImpactsRanking" type="boolean" default="false"/>
    
    <aura:attribute name="valCostmin" type="Integer" default="0"/>
    <aura:attribute name="valCostmax" type="Integer" default="100"/>
    <aura:attribute name="valpourcentagemin" type="Integer" default="0"/>
    <aura:attribute name="valpourcentagemax" type="Integer" default="100"/>
    
       <table wtdth="100%">
         <caption>Probility Ranking</caption>
          <thead>
	         <tr>
			    <th wtdth="34%">Probability</th>
			    <th wtdth="33%">Pourcentage</th>
			    <th wtdth="33%">Description</th>
			  </tr>
		  </thead>
		  		<aura:if isTrue="{!v.showProbilityRanking == true}">
		  		 <tbody>
	          <tr>
		        <td  style="background-color:red" wtdth="34%"></td>
		        <td  style="background-color:green">
		                <div>
					        <div class="label">{!v.label}</div>
						    <div aura:id="slider" id="slider" class="slider"/>
					    </div>
		         </td>
		         <td  style="background-color:yellow"></td>
	          </tr>
           </tbody>
           <aura:set attribute="else">
<!--           le cas oÃ¹ !v.showProbilityRanking==false  -->
           </aura:set>
		  		</aura:if>
		  		
		 
           
         </table>
         <table>
         <tr class="{!v.showHsseImpactsRanking == true ? 'showBloc' : 'hideBloc'}">
            <td class="tdClass">
               <H1>HSSE Impacts Ranking</H1>
		       <div class="slds-size--1-of-1">       
			        <div class="label">{!v.label}</div>
				    <div aura:id="slider3" class="slider3" id="slider3"/>
			    </div>
            </td>
         </tr>
       </table>
		    
			        
				    
		    
		      
		     
			       
		    
		    
		    <aura:if isTrue="{!v.showBusinessImpactsRanking}"> 
		       <H1>Business Impacts Ranking</H1>
		       
		    </aura:if>   
	  
	    <lightning:icon class="iconPrev" onclick="{!c.PrevPage}" iconName="utility:chevronleft" size="large" />
	    <lightning:icon class="iconNext" onclick="{!c.nextPage}" iconName="utility:chevronright" size="large" />
    
</aura:component>